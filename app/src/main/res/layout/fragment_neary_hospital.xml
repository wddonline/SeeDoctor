<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="org.wdd.app.android.seedoctor.ui.hospital.fragment.NearbyHospitalFragment">

    <android.support.v7.widget.Toolbar
        android:id="@+id/fragment_nearby_hospital_toolbar"
        android:layout_width="match_parent"
        android:layout_height="?actionBarSize"
        android:background="@drawable/title_background"/>

    <org.wdd.app.android.seedoctor.views.SDViewPager
        android:id="@+id/fragment_nearby_hospital_viewpager"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
</LinearLayout>
